#include <opencv2/core/core.hpp>
#include <opencv2/highgui/highgui.hpp>
#include <opencv2/imgproc/imgproc.hpp>

int main(int argc, char ** argv) {
	cv::Mat image = cv::imread("opencv_screenshot.jpg");
	cv::imshow("Framme OpenCV", image);
	cv::waitKey(0);
}



